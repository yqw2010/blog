<div class="container message">
	<div class="cate-wraper post">
		<h2>
      <%- partial("../_partial/post/rss.ejs") %>
			<a href="/"><%- config.author %></a> » 友情链接
		</h2>
		<ul>
		<% var friends = site.data.text.friends;
				Object.keys(friends).forEach(function(name){ %>
			<li><a href="<%- friends[name] %>" target="_blank"><%- name %></a></li>
		<% }) %>
		</ul>
		<%- partial("page_footer") %>
	</div>
</div>
